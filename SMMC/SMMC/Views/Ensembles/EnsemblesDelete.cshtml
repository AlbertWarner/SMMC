@model SMMC.Models.Ensembles

@{
    ViewData["Title"] = "Ensembles Delete";
}
<h2>Delete</h2>
<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Ensembles</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Ensemble Name
        </dt>
        <dd>
            @Html.DisplayFor(model => model.EnsembleName)
        </dd>
    </dl>

    <form asp-action="EnsemblesDelete">
        <input type="hidden" asp-for="EnsembleID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="EnsemblesDeleteView">Back to Delete List</a>
    </form>
</div>