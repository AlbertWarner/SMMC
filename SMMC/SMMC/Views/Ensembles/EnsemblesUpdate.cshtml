@model SMMC.Models.Ensembles

@{
    ViewData["Title"] = "Update Ensembles Info";
}
<h2>Update</h2>
<h4>Ensembles</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EnsemblesUpdate">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EnsembleID" />
            <div class="form-group">
                <label asp-for="EnsembleName" class="control-label">Ensemble Name</label>
                <input asp-for="EnsembleName" class="form-control" />
                <span asp-validation-for="EnsembleName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-action="EnsemblesUpdateView">Back to Update List</a>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}